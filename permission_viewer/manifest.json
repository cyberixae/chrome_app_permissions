{
  "manifest_version": 2,

  "name": "Permission Viewer",
  "description": "Review permissions of installed apps and extensions.",
  "version": "1.1",
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Permission Viewer",
    "default_popup": "main.html"
  },
  "permissions": [
    "management"
  ],
  "icons": {
    "16": "icon_16.png",
    "128": "icon_128.png"
  }
}
